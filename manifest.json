{
  "manifest_version": 2,
  "name": "EthVision",
  "description": "An extension for seeing Ethereum price data.",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "img/eth-16.png",
    "default_popup": "popup.html",
    "default_title": "EthVision"
  },
  "icons": { 
  	"16": "img/eth-16.png",
    "48": "img/eth-48.png",
    "128": "img/eth-128.png"
  },
  "permissions": [
    "storage"
  ]
}